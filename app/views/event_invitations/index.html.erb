<h1>My Upcoming Events</h1>
<br>


<% if @event_invitations != []%>
  <div class="row">
    <% @event_invitations.each do |event| %>
      <%= render partial: 'events/event_tile', locals: { event: event, a: "upcoming_events"} %>
    <% end %>
  </div>

<%else %>
  <p>You haven't been invited to an event yet :(</p>
<% end %>

<br>

