<p id="notice"><%= notice %></p>

<h1>Event Invitations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <% if @event_invitations %>
    <% @event_invitations.each do |event| %>
      <tr>
        <td><%= link_to event.title, event_path(event.id) %></td>

      </tr>
    <% end %>
  <%end %>
  </tbody>
</table>

<br>

