<p id="notice"><%= notice %></p>

<h1>Organization Invitations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <% if @organization_invited %>
    <% @organization_invited.each do |organization| %>
      <tr>
        <td><%= link_to organization.name, organization_path(organization.id) %></td>

      </tr>
    <% end %>
  <%end %>
  </tbody>
</table>

<br>

